    attribute type { xsd:Name }?,
    empty
  }
expan =
  
  ## (expansion) contains the expansion of an abbreviation.
  element tei:expan {
    macro.phraseSeq,
    att.global.attributes,
    att.editLike.attributes,
    empty
  }
ptr =
  
